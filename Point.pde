public class Point{
  public double x,y;
  public Point(double x, double y){
    this.x = x;
    this.y = y;
  }
  
  public float getX(){
    return (float)x;
  }
  
  public float getY(){
    return (float)y;
  }  
  
  public void setX(double x){
    this.x = x;
  }
  
  public void setY(double y){
    this.y = y;
  }  
}  
